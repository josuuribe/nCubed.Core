<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
    <Authors>Josu Uribe</Authors>
    <Company>GFI</Company>
    <PackageProjectUrl>https://josuuribe.github.io/nCubedCore/</PackageProjectUrl>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/josuuribe/nCubedCore</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <NeutralLanguage />
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <PackageTags>GFI nCubed EFCore .NET Core</PackageTags>
    <Description>This is a Fluent API library for the most common EFCore functions using Repository pattern.</Description>
    <PackageId>nCubed.EFCore</PackageId>
    <Product>nCubedCore</Product>
    <PackageIconUrl>https://josuuribe.github.io/nCubedCore/images/nCubedCore.ico</PackageIconUrl>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="1.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="1.1.0" />
    <PackageReference Include="SourceLink.Create.CommandLine" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.Linq.Dynamic.Core" Version="1.0.0" />
    <PackageReference Include="System.Security.Principal.Windows" Version="1.0.0" />
  </ItemGroup>
</Project>
